{
  "x_spi_schema_url": "https://specs.frictionlessdata.io/table-schema",
  "fields": [
    {
      "name": "ACE_Sample_Number [string]",
      "type": "string",
      "format": "default",
      "title": "ACE sample number",
      "description": "unique sample code from ACE, formatted as follows: shipname/ACE/legnumber/projectnumber/julianday[UTC]/eventnumber/PIinitials/projectsampleID"
    },
    {
      "name": "project_sample_number [string]",
      "type": "string",
      "format": "default",
      "title": "Project sample number",
      "description": "ACE project 1 sample ID code"
    },
    {
      "name": "Leg [string]",
      "type": "string",
      "format": "default",
      "title": "Leg number",
      "description": "Leg of ACE voyage during which sample was collected, 1-3"
    },
    {
      "name": "Sampling_DateTime_UTC [YYYY-MM-DDTHH:MM:SS+HH:MM]",
      "type": "datetime",
      "format": "%Y-%m-%dT%H:%M:%S%z",
      "title": "Date time of sampling",
      "description": "date and time at which the underway seawater sample was collected in ISO8601 format YYYY-MM-DDTHhh:mm:ss+00:00 [UTC]",
      "unit": "UTC"
    },
    {
      "name": "Latitude [decimal_deg_N]",
      "type": "number",
      "format": "default",
      "title": "Latitude",
      "description": "Latitude of seawater sample collection [degree_north]",
      "unit": "degree_north",
      "x_spi_cf_standard_name": "latitude",
      "x_spi_cf_unit": "degree_north",
      "x_spi_cf_attribute": "Latitude of seawater sample collection"
    },
    {
      "name": "Longitude [decimal_deg_E]",
      "type": "number",
      "format": "default",
      "title": "Longitude",
      "description": "Longitude of seawater sample collection [degree_east]",
      "unit": "degree_east",
      "x_spi_cf_standard_name": "longitude",
      "x_spi_cf_unit": "degree_east",
      "x_spi_cf_attribute": "Longitude of seawater sample collection"
    },
    {
      "name": "Depth [m]",
      "type": "number",
      "format": "default",
      "title": "depth",
      "description": "Depth sample was collected from [metres]",
      "unit": "metres",
      "x_spi_cf_standard_name": "depth",
      "x_spi_cf_unit": "m",
      "x_spi_cf_attribute": "Depth from which sample was collected"
    },
    {
      "name": "Volume [L]",
      "type": "number",
      "format": "default",
      "title": "Volume",
      "description": "Volume of seawater filtered [litres]",
      "unit": "litres"
    },
    {
      "name": "Replicate_Flag [string]",
      "type": "integer",
      "format": "default",
      "title": "Replicate flag",
      "description": "Flag indicating number of replicates averaged [no units]"
    },
    {
      "name": "EA_Flag_C [string]",
      "type": "integer",
      "format": "default",
      "title": "EA flag Carbon",
      "description": "Carbon detection voltage flag [no units]"
    },
    {
      "name": "EA_Flag_N [string]",
      "type": "integer",
      "format": "default",
      "title": "EA flag Nitrogen",
      "description": "Nitrogen detection voltage flag [no units]"
    },
    {
      "name": "He_Flag [string]",
      "type": "integer",
      "format": "default",
      "title": "Helium dilution flag",
      "description": "Helium dilution flag [no units]"
    },
    {
      "name": "POC [uM]",
      "type": "number",
      "format": "default",
      "title": "Particulate organic carbon concentration",
      "description": "Particulate organic carbon concentration [micromolar]",
      "unit": "micromolar"
    },
    {
      "name": "PON [uM]",
      "type": "number",
      "format": "default",
      "title": "Particulate organic nitrogen concentration",
      "description": "Particulate organic nitrogen concentration [micromolar]",
      "unit": "micromolar"
    },
    {
      "name": "delta13C [o/oo]",
      "type": "number",
      "format": "default",
      "title": "Delta 13 C",
      "description": "delta 13 C carbon stable isotope composition ratio of 13C to 12C [parts per thousand]",
      "unit": "parts per thousand"
    },
    {
      "name": "delta15N [o/oo]",
      "type": "number",
      "format": "default",
      "title": "Delta 15 N",
      "description": "delta 15 N nitrogen stable isotope composition ratio of 15N to 14N [parts per thousand]",
      "unit": "parts per thousand"
    }
  ],
  "missingValues": [
    "NaN"
  ]
}